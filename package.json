{
  "name": "vanillajs-components",
  "description": "Simple to use modal / alert / dialog / popup / toast / overlay / notification. Created without any framework. No javascript knowledge required! Works on every browser and device! IE >= 9",
  "version": "1.0.1",
  "main": "dist/bundle.cjs.js",
  "module": "dist/bundle.esm.js",
  "unpkg": "dist/bundle.umd.js",
  "sideEffects": false,
  "keywords": [
    "ios",
    "modal",
    "alert",
    "dialog",
    "vanilla javascript",
    "vanilla js",
    "popup",
    "prompt",
    "confirm",
    "toaster",
    "notifications"
  ],
  "files": [
    "dist/*"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "prepare": "rollup --config",
    "prebuild:lib": "rm -rf dist",
    "build:lib": "rollup -c",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@babel/preset-env": "^7.16.11",
    "@vitejs/plugin-vue": "^2.0.0",
    "eslint": "^8.7.0",
    "eslint-plugin-vue": "^8.4.0",
    "rollup": "^2.66.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "vite": "^2.7.2",
    "vue": "^3.2.25"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/grim-reapper/vanillajs-components.git"
  }
}
